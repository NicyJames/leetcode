class Solution {
public:
    string gcdOfStrings(string str1, string str2) {
        if((str1+str2)!=(str2+str1))
        {
            return "";
        }
        else
        //basically here from string 1 u r trying to find the substring
        //starting from 0th index,till the index(gcd of str1 and str2)
        {
            return str1.substr(0,gcd(str1.size(),str2.size()));
        }
    }
};
